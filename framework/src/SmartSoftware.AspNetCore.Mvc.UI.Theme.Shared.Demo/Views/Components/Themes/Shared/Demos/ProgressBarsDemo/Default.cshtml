@using SmartSoftware.AspNetCore.Mvc.UI.Theme.Shared.Demo.Views.Components.Themes.Shared.Demos.ProgressBarsDemo

<ss-component-demo-section title="Example" view-path="@ProgressBarsDemoViewComponent.ViewPath">
    <ss-progress-bar value="70" />

    <ss-progress-bar type="Warning" value="25"> %25 </ss-progress-bar>

    <ss-progress-bar type="Success" value="40" strip="true" />

    <ss-progress-bar type="Dark" value="10" min-value="5" max-value="15" strip="true"> %50 </ss-progress-bar>

    <ss-progress-group>
        <ss-progress-part type="Success" value="25" />
        <ss-progress-part type="Danger" value="10" strip="true"> %10 </ss-progress-part>
        <ss-progress-part type="Primary" value="50" animation="true" strip="true" />
    </ss-progress-group>
</ss-component-demo-section>
